  "dependencies": {
    "babel-env": "^2.4.1",
    "babel-eslint": "^8.2.3",
    "babel-register": "^6.26.0",
    "cors": "^2.8.5",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "express": "^4.17.3",
    "jest": "^23.0.1",
    "morgan": "^1.9.1",
    "pg": "^7.6.1",
    "request-ip": "^2.1.3",
    "require-dir": "^1.0.0"
